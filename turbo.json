{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["graphql-codegen", "^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "dev": {
      "dependsOn": ["graphql-codegen"],
      "cache": false,
      "persistent": true
    },
    "graphql-codegen": {
      "dependsOn": ["^graphql-codegen"]
    }
  }
}
